package com.example.service;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.TreeSet;

public class Emp_Service {

	public static void main(String[] args) {
		List<Integer> arrayList = null;
		Integer intArr [] =  {1,2,3,4,5,6,4,3,2,4,2,1};
		List<Integer> asList = Arrays.asList(intArr);
		TreeSet<Integer> ts = new TreeSet<>(asList);
		for(Integer res : ts) {
			arrayList = new ArrayList<>();
			arrayList.add(res);
			
		}
		System.out.println("ArrayList : >"+arrayList.indexOf(3));
	}
}